    "multer": "^1.4.5-lts.1",
    "nodemon": "^3.1.4",
    "swagger-jsdoc": "^6.2.8",
    "swagger-ui-express": "^5.0.1",
    "winston": "^3.18.3",
    "winston-papertrail": "^1.0.5",
    "winston-transport": "^4.9.0",
    "xlsx": "^0.18.5",
    "yamljs": "^0.3.0"
