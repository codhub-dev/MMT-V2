{
  "name": "nalaso-express",
  "version": "1.1.1",
  "bin": "cli.js",
  "scripts": {
    "start": "nodemon ./bin/www",
    "generate:swagger": "node -e \"console.log(JSON.stringify(require('./docs/swagger')));\" > swagger.json",
    "seed:test-users": "node seed-test-users.js",
    "artillery:test": "artillery run test.yml",
    "artillery:test:record": "npm run seed:test-users && artillery run test.yml --record --key a9_je5rl38330pw43a4lisr7hwl4g9ztxqi",
