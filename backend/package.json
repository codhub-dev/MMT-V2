    "artillery:load:record": "npm run seed:test-users && artillery run artillery-load-test.yml --record --key a9_je5rl38330pw43a4lisr7hwl4g9ztxqi",
    "artillery:spike": "artillery run artillery-spike-test.yml",
    "artillery:spike:record": "npm run seed:test-users && artillery run artillery-spike-test.yml --record --key a9_je5rl38330pw43a4lisr7hwl4g9ztxqi",
    "artillery:report": "artillery report report.json"
  },
  "dependencies": {
